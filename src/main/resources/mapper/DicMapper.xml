        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.product.mapper.DicMapper">
    <resultMap id="DicMb" type="java.util.Map">
        <result column="CODE" property="code"/>
        <result column="NAME" property="name"/>
